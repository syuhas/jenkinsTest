pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..test line'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..Added this message to test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....test 3'
            }
        }
    }
}